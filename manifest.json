{
  "name": "Article Grabber",
  "icons": { "128": "icon.png" },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_title": "Grab Article",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "manifest_version": 2,
  "version": "1.0"
}
